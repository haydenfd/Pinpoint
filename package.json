{
  "name": "llm-bookmarks",
  "displayName": "Pinpoint",
  "version": "0.1.0",
  "description": "A lightweight bookmark manager for saving and revisiting important LLM chat messages.",
  "author": "Hayden D'Souza <haydenfds@gmail.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.15.0",
    "lucide-react": "^0.563.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/chrome": "0.0.258",
    "@types/node": "^20.19.0",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.23",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^24.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.19",
    "typescript": "5.3.3",
    "vitest": "^2.1.8"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "commands": {
      "toggle-sidebar": {
        "suggested_key": {
          "default": "Ctrl+Shift+P",
          "mac": "Command+Shift+P"
        },
        "description": "Toggle LLM Bookmarks sidebar"
      }
    }
  }
}
